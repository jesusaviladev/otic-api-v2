{
  "name": "otic-api-v2",
  "version": "1.0.0",
  "description": "REST API for OTIC project",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose --silent",
    "test:watch": "npm run test -- --watch",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
