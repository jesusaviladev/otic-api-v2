  },
  "author": "jesusaviladev",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.1",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "express": "4.17.2",
    "express-validator": "6.14.0",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "6.15.1"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "jest": "27.5.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.2.2"
